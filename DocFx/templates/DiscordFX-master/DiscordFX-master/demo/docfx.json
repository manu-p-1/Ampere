{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "api/**.md",
          "api/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appName": "ACME",
      "_appTitle": "ACME Documentation",
      "_enableSearch": true,
      "_disableAffix": true,
      "_appFooter": "ACME Inc.<br/>Generated by <b>DocFX</b>",
      "_disableSideFilter": true,
      "_gitContribute": {
        "repo": "https://github.com/jbltx/DiscordFX"
      },
      "_gitUrlPattern": "github"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "../discordfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}